                  p.profile_sidebar_title FILTER KITS BY
                  div.profile_sidebar_options
                    button.profile_sidebar_button.md-button.md-default-theme
                      span.ng-scope All
                    button.profile_sidebar_button.md-button.md-default-theme
                      span.ng-scope Online
                    button.profile_sidebar_button.md-button.md-default-theme
                      span.ng-scope Offline

                div.profile_content_main(flex="")
                  div.profile_content_main_top
                    a.myProfile_addKitButton.md-button.md-default-theme
                      span.ng-scope ADD A NEW KIT
                    span.myProfile_content_main_title.ng-binding

                  div.ng-scope
                    a.kitList.kitList_primary.md-button.md-default-theme(href="/smart-citizen/profile/kits/1", layout="column", layout-align="start start")
                      h4.ng-binding.ng-scope Testing kit
                      p.ng-binding.ng-scope Penrhyn Bay, United Kingdom

                  div.ng-scope
                    a.kitList.kitList_primary.md-button.md-default-theme.kitList_borderBottom(href="/smart-citizen/profile/kits/2", layout="column", layout-align="start start")
                      h4.ng-binding.ng-scope john rees smart citizen
                      p.ng-binding.ng-scope Barcelona, Spain